#!/usr/bin/env node
var VERSION = '0.1.0';

var scraper = require('../lib/scraper');

var program = require('commander');

console.log('jkl-event-scraper ' + VERSION + '\n');

program.
    version(VERSION).
    option('-o --output <output>', 'output file').
    option('-s --silent', 'silent').
    parse(process.argv);

scraper.scrape(program);
